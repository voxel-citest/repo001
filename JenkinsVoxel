#!/usr/bin/env groovy
@Library('voxel-citest/jsl')_

pipeline {
	agent{ 
		label "Linux-Infra"
	}
	environment {
		GREETINGS = "Hello Jenkins!"
	}
	stages{
		stage("Test"){
			steps{
				echo "Testing"
			}
		}
		stage("Call Script"){
			steps{
				sh 'chmod +x ./script.sh'
				sh './script.sh'
			}
		}
		stage("Greetings"){
			steps{
				echo "${env.GREETINGS}"
				NotifyBySlack(buildStatus:"CHECK");
			}
		}
	}
}
